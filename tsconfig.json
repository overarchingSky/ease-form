{
    "compileOnSave": true,
    "compilerOptions": {
        "sourceMap": true, //代码映射
        "noImplicitAny": false, //强类型检查
        "module": "es2015", //组织代码方式
        "target": "es2015", //编译目标平台
        "allowJs": true, //允许使用js
        "strict": false,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true //允许从没有设置默认导出的模块中默认导入。这并不影响代码的显示，仅为了类型检查。
    }
}