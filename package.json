{
    "name": "ease-form",
    "version": "1.10.2",
    "description": "render form-item easily",
    "author": "leezng <leezng@outlook.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/overarchingSky/ease-form.git"
    },
    "keywords": [
        "vue",
        "config",
        "dynamic",
        "form",
        "mobile",
        "pc",
        "phone",
        "drag"
    ],
    "files": [
        "src",
        "dist",
        "yarn.lock"
    ],
    "main": "dist/ease-form.umd.js",
    "module": "dist/ease-form.esm.js",
    "unpkg": "dist/ease-form.min.js",
    "browser": {
        "./sfc": "src/ease-form.vue"
    },
    "scripts": {
        "rel": "standard-version",
        "dev": "vue-styleguidist server",
        "doc": "vue-styleguidist build",
        "build": "npm run build:unpkg & npm run build:es & npm run build:umd & npm run doc",
        "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/ease-form.umd.js",
        "build:es": "rollup --config build/rollup.config.js --format es --file dist/ease-form.esm.js",
        "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/ease-formr.min.js",
        "stdver": "standard-version -m '[skip ci] chore(release): v%s'",
        "release": "gren release --override",
        "test": "jest"
    },
    "homepage": "https://github.com/overarchingSky/ease-form",
    "dependencies": {
        "@types/vue": "^2.0.0",
        "awesome-typescript-loader": "^5.2.1",
        "element-ui": "^2.11.1",
        "less": "^3.9.0",
        "less-loader": "^5.0.0",
        "lodash.get": "^4.4.2",
        "lodash.set": "^4.3.2",
        "rollup-plugin-typescript2": "^0.22.1",
        "ts-loader": "^6.0.4",
        "typescript": "^3.5.3",
        "vue-codemirror": "^4.0.6",
        "webpack-cli": "^3.3.6"
    },
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@babel/plugin-transform-runtime": "^7.4.3",
        "@babel/preset-env": "^7.4.3",
        "@femessage/upload-to-ali": "^1.8.0",
        "@vue/test-utils": "^1.0.0-beta.29",
        "babel-loader": "^8.0.5",
        "file-loader": "^3.0.1",
        "github-release-notes": "^0.17.0",
        "glob": "^7.1.3",
        "husky": "1.3.1",
        "jest": "^24.8.0",
        "lint-staged": "^8.1.0",
        "minimist": "^1.2.0",
        "prettier": "1.18.2",
        "rollup": "^1.9.0",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-commonjs": "^9.3.4",
        "rollup-plugin-terser": "^4.0.4",
        "rollup-plugin-vue": "^4.7.2",
        "standard-version": "^6.0.1",
        "stylelint": "^9.10.0",
        "stylelint-config-standard": "^18.2.0",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "vue": "^2.6.10",
        "vue-loader": "^15.7.1",
        "vue-styleguidist": "^3.16.3",
        "vue-template-compiler": "^2.5.16",
        "webpack": "^4.38.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">= 4.0.0",
        "npm": ">= 3.0.0"
    },
    "vue-sfc-cli": "1.10.4",
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "post-commit": "git update-index --again"
        }
    },
    "lint-staged": {
        "*.(js|md)": [
            "prettier --write",
            "git add"
        ],
        "*.vue": [
            "prettier --write",
            "stylelint --fix",
            "git add"
        ]
    }
}
